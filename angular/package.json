{
  "name": "mythaistar-restaurant",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -pc proxy.conf.json -o",
    "test": "ng test --browsers Chrome",
    "test:ci": "ng test --browsers ChromeHeadless --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "serve": "ng serve --open --environment=dev",
    "serve:aot": "ng serve --open --environment=dev --aot",
    "serve:pwa": "npm run build:pwa && http-server dist -p 8080",
    "serve:prod": "ng serve --open --environment=prod",
    "serve:prod:aot": "ng serve --open --environment=prod --aot",
    "serve:prodcompose": "ng serve --open --environment=prodcompose",
    "serve:prodcompose:aot": "ng serve --open --environment=prodcompose --aot",
    "serve:node": "ng serve --open --environment=node",
    "serve:node:aot": "ng serve --open --environment=node --aot",
    "build": "ng build --environment=dev --aot",
    "build:pwa": "ng build --environment=pwa --prod --build-optimizer && npm run ngsw-config && npm run ngsw-copy",
    "build:prod": "ng build --environment=prod --prod --build-optimizer",
    "build:prodcompose": "ng build --environment=prodcompose --prod --build-optimizer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "5.0.0-rc0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "5.0.0-rc0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angular/service-worker": "^5.0.0",
    "@covalent/core": "1.0.0-rc.0",
    "@covalent/dynamic-forms": "1.0.0-rc.0",
    "@covalent/highlight": "1.0.0-rc.0",
    "@covalent/http": "1.0.0-rc.0",
    "@covalent/markdown": "1.0.0-rc.0",
    "angular2-moment": "^1.7.0",
    "apollo-angular": "^0.13.0",
    "apollo-client": "^1.1.1",
    "core-js": "^2.4.1",
    "graphql-tag": "^2.0.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.11.0",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "md2": "^0.0.29",
    "moment": "^2.18.1",
    "rxjs": "^5.5.2",
    "showdown": "^1.6.4",
    "whatwg-fetch": "^2.0.3",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0-beta.2",
    "@angular/compiler-cli": "^5.0.0",
    "@types/hammerjs": "2.0.30",
    "@types/jasmine": "^2.5.47",
    "@types/node": "~6.0.60",
    "@types/selenium-webdriver": "2.53.36",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-firefox-launcher": "^1.0.0",
    "protractor": "~5.1.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "optionalDependencies": {
    "@covalent/data": "^0.7.1"
  }
}
